import referentieHeroHome from "../images/referentie-hero-home.png";
import nickNewfront from "../images/nick-newfront.jpg";
import checkIcoon from "../images/icons/check-icoon.svg";

import headzLogo from "../images/referrals/headz-logo.svg";
